    <gndo:gndIdentifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string"><!-- GND ID --></gndo:gndIdentifier>
    
    <!-- Mandatory -->
    <gndo:preferredNameForThePlaceOrGeographicName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"><!-- Preferred Name --></gndo:preferredNameForThePlaceOrGeographicName>
    
    <!-- Optional -->
    <gndo:variantNameForThePlaceOrGeographicName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"><!-- Variant or Alternative Name --></gndo:variantNameForThePlaceOrGeographicName>
    
    <!-- Geo Data -->
    <gndo:geographicAreaCode rdf:resource="http://d-nb.info/standards/vocab/gnd/geographic-area-code#nnnn"/>    
    <geo:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral"><!-- Geo Data (Lat, Long) --></geo:asWKT>
    <!-- Geo Type -->
    <rdf:type rdf:resource="http://www.opengis.net/ont/sf#Point"/>
  
  </rdf:Description>
  
  <!-- GND Key DataSet Local Copy END -->
  
  <!-- WIKIDATA Key DataSet Local Copy START -->
  <rdf:Description rdf:about="http://www.wikidata.org/entity/Qnnnn#local">
    <schema:about rdf:resource="http://www.wikidata.org/entity/Qnnnn"/>
    <cc:license rdf:resource="http://creativecommons.org/publicdomain/zero/1.0/"/>
    <schema:dateModified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime"><!-- yyyy-mm-ddThh:mm:ssZ --></schema:dateModified>
    
    <!-- Mandatory -->
    <!-- Name Label -->
    <rdfs:label xml:lang="de"><!-- Name --></rdfs:label>
    <rdfs:label xml:lang="en"><!-- Name --></rdfs:label>
    
    <!-- Mandatory -->
    <!-- Geo Data -->
    <wdt:P625 rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral"><!-- Geo Data (Lat, Long) --></wdt:P625>
    <wdt:P625 rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral"><!-- Geo Data (Lat, Long) --></wdt:P625>    
    <wdt:P2044 rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal"><!-- Geo Data (Alt) --></wdt:P2044>
    <wdt:P2044 rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal"><!-- Geo Data (Alt) --></wdt:P2044>
    
  </rdf:Description>
