<rdf:RDF xmlns:bf="http://id.loc.gov/ontologies/bibframe/" xmlns:bibo="http://purl.org/ontology/bibo/" xmlns:crm="http://www.cidoc-crm.org/cidoc-crm/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dct="http://purl.org/dc/terms/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:decomp="http://www.w3.org/ns/lemon/decomp#" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:gnd="http://d-nb.info/gnd/" xmlns:gndo="http://d-nb.info/standards/elementset/gnd#" xmlns:lexinfo="http://www.lexinfo.net/ontology/2.0/lexinfo#" xmlns:marc21="http://www.loc.gov/MARC21/slim" xmlns:mhdbdb="http://www.mhdbdb.sbg.ac.at/" xmlns:oa="http://www.w3.org/ns/oa#" xmlns:ontolex="http://www.w3.org/ns/lemon/ontolex#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdac="http://rdaregistry.info/Elements/c/" xmlns:rdau="http://rdaregistry.info/Elements/u/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:schema="http://schema.org/" xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:srw="http://www.loc.gov/zing/srw/" xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:time="http://www.w3.org/2006/time#" xmlns:wdt="http://www.wikidata.org/prop/direct/" xmlns:wikidata="http://www.wikidata.org/entity/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:wgs84_pos="http://www.w3.org/2003/01/geo/wgs84_pos#" xmlns:cc="http://creativecommons.org/ns#" xmlns:geo="http://www.opengis.net/ont/geosparql#" xmlns:sf="http://www.opengis.net/ont/sf#">
  
  
  <!-- Place START -->
  <crm:E53_Place rdf:about="http://dhPLUS.sbg.ac.at/places/nnnn">
    <owl:sameAs rdf:resource="http://www.wikidata.org/entity/Qnnnn"/>
    <dc:source rdf:resource="http://www.wikidata.org/entity/Qnnnn"/>
    <crm:P87_is_identified_by>
      <crm:E48_Place_Name>
          <!-- Mandatory -->
          <gndo:preferredNameForThePlaceOrGeographicName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"><!-- Preferred Place Name --></gndo:preferredNameForThePlaceOrGeographicName>
          <gndo:preferredNameForThePlaceOrGeographicName>
              <rdf:Description>
                  <dcterms:isReplacedBy>
                      <rdf:Description>
                          <gndo:preferredNameForThePlaceOrGeographicName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"><!-- Corrected Preferred Place Name --></gndo:preferredNameForThePlaceOrGeographicName>
                      </rdf:Description>              
                  </dcterms:isReplacedBy>
                  <!-- OR -->
                  <!-- No Correction -->
                  <dcterms:isReplacedBy/>
              </rdf:Description>
          </gndo:preferredNameForThePlaceOrGeographicName>
          
          <!-- Optional -->
          <rdfs:label xml:lang="en"><!-- Place Name --></rdfs:label>
          
          <!-- Optional -->
          <gndo:variantNameForThePlaceOrGeographicName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"><!-- Variant or Alternative Place Name --></gndo:variantNameForThePlaceOrGeographicName>
      </crm:E48_Place_Name>
    </crm:P87_is_identified_by>
      
    <crm:P87_is_identified_by>
      <crm:E47_Spatial_Coordinates>
        <!-- REST API's for Geo Data:  -->
          <!-- 1. Nominatim for Open Street Map Data: 2D, Output Format Geojson -->
          <!-- 2. ARC GIS: 3D, Output Format ESRI JSON -->
                    
          <!-- Point -->
        <!-- Linking CIDOC to GEO Ontology 1. SOLUTION -->          
        <!-- dhPLUS Ontology added with: 
        <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/sf#hasGeometry">
        <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E47_Spatial_Coordinates"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/></owl:DatatypeProperty>
        -->
        <!-- Linking CIDOC to GEO Ontology 2. SOLUTION -->
        <!-- Global Property: rdfs:isDefinedBy -->
        
        <!-- 1. SOLUTION -->
        <sf:hasGeometry>
          <sf:Point>
              <sf:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">
                  <!-- 2D -->
                  POINT(x y)
                  <!-- OR -->
                  <!-- 3D -->
                  POINT(x y z)              
            </sf:asWKT>
          </sf:Point>
        </sf:hasGeometry>
        <!-- OR -->
        <!-- Curve -->
        <sf:hasGeometry>
            <sf:Curve>
                <sf:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">
                    <!-- 2D -->
                    <!-- Line with 3 Points -->
                    LINESTRING(x1 y1, x2 y2, x3 y3)
                    <!-- OR -->
                    <!-- 3D -->
                    <!-- Line with 2 Points -->
                    LINESTRING(x1 y1 z1, x2 y2 z2)              
                </sf:asWKT>
            </sf:Curve>
        </sf:hasGeometry>
        <!-- OR -->
        <!-- Surface -->
        <sf:hasGeometry>            
            <sf:Surface>
                <sf:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">
                    <!-- 2D -->
                    POLYGON((x1 y1, x2 y2, x3 y3, x1 y1))
                    <!-- OR -->
                    <!-- 3D -->                      
                    POLYGON((x1 y1 z1, x2 y2 z2, x3 y3 z3, x1 y1 z1))              
                </sf:asWKT>
            </sf:Surface>
        </sf:hasGeometry>
      </crm:E47_Spatial_Coordinates>
    </crm:P87_is_identified_by>
  </crm:E53_Place>
  <!-- Place END -->

  <!-- Key DataSets START -->
  <!-- GND Key DataSet Local Copy START -->
  <rdf:Description rdf:about="http://d-nb.info/gnd/nnnn#local">
