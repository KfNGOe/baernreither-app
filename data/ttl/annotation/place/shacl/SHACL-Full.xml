    <ns0:targetSubjectsOf rdf:resource="http://www.w3.org/ns/shacl#hasValue"/>
    <ns0:targetSubjectsOf rdf:resource="http://www.w3.org/ns/shacl#ignoredProperties"/>
    <ns0:targetSubjectsOf rdf:resource="http://www.w3.org/ns/shacl#in"/>
