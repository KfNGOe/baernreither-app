                                                        </rdf:first>

                                                        <rdf:rest>
                                                          <rdf:Description>
                                                            <rdf:first>
                                                              <rdf:Description>
                                                                <ns0:nodeKind rdf:resource="http://www.w3.org/ns/shacl#BlankNode"/>
                                                                <ns0:closed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</ns0:closed>
                                                                <ns0:property>
                                                                  <rdf:Description>
                                                                    <ns0:path rdf:resource="http://www.w3.org/ns/shacl#zeroOrOnePath"/>
                                                                    <ns0:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</ns0:minCount>
