                                              </rdf:Description>
                                            </rdf:rest>

                                          </rdf:Description>
                                        </rdf:first>

                                        <rdf:rest>
                                          <rdf:Description>
                                            <rdf:first>
                                              <rdf:Description>
                                                <rdf:first rdf:resource="http://www.w3.org/ns/shacl#alternativePath"/>
                                                <rdf:rest>
