<?xml version="1.0" encoding="utf-8" ?>

<!DOCTYPE RDF [    
    <!ENTITY xml "http://www.w3.org/XML/1998/namespace"> 
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#"> 
    <!ENTITY owl "http://www.w3.org/2002/07/owl#">
    <!ENTITY schema "http://schema.org/">
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
    <!ENTITY cc "http://creativecommons.org/ns#">
    <!ENTITY dc "http://purl.org/dc/elements/1.1/">        
    <!ENTITY dcterms "http://purl.org/dc/terms/"> 
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/">
    <!ENTITY wdrs "http://www.w3.org/2007/05/powder-s#">
    <!ENTITY premis "http://www.loc.gov/premis/rdf/v1#">
    <!ENTITY crm "http://www.cidoc-crm.org/cidoc-crm/">
    <!ENTITY gndo "http://d-nb.info/standards/elementset/gnd#">
    <!ENTITY wikibase "http://wikiba.se/ontology#">
    <!ENTITY wd "http://www.wikidata.org/entity/">
    <!ENTITY wdt "http://www.wikidata.org/prop/direct/">
    <!ENTITY geo "http://www.opengis.net/ont/geosparql#">
    <!ENTITY sf "http://www.opengis.net/ont/sf#">
    <!ENTITY sh "http://www.w3.org/ns/shacl#">
    <!ENTITY dhplusPlace "https://dhplus.sbg.ac.at/place/">
    
]>

<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:xsd="&xsd;" xmlns:schema="&schema;" xmlns:skos="&skos;" xmlns:cc="&cc;" xmlns:dc="&dc;" xmlns:dct="&dcterms;" xmlns:foaf="&foaf;" xmlns:wdrs="&wdrs;" xmlns:premis="&premis;" xmlns:crm="&crm;" xmlns:gndo="&gndo;" xmlns:wikibase="&wikibase;" xmlns:wd="&wd;" xmlns:wdt="&wdt;" xmlns:sf="&sf;" xmlns:geo="&geo;" xmlns:sh="&sh;" xmlns:dhplusPlace="&dhplusPlace;">
    
    <rdf:Description rdf:about="https://dhplus.sbg.ac.at/shapes/place/">
        <rdf:type rdf:resource="http://owl.org/Ontology"/>
        <dc:title>dhPLUS Place SHACL</dc:title>
        <dc:creator>Richard HÃ¶rmann</dc:creator>
        <owl:comment xml:lang="en">This is a SHACL graph for validating the dhPLUS OWL ontology.</owl:comment>
        <owl:imports rdf:resource=""/>
        <!-- <sh:entailment rdf:resource="http://www.w3.org/ns/entailment/RDFS"/> -->
    </rdf:Description>
    
    
    <!-- Mash DataSets START -->
    <!-- GND DataSet START -->
    <rdf:Description rdf:about="&dhplusPlace;Gnd">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdf:type rdf:resource="&sh;NodeShape"/>
        <sh:property>
            <rdf:Description>
                <sh:path rdf:resource="&dhplusPlace;hasPart"/>
                <sh:node rdf:resource="&gndo;TerritorialCorporateBodyOrAdministrativeUnit"/>
            </rdf:Description>
        </sh:property>                
    </rdf:Description>
    
    <rdf:Description rdf:about="&gndo;TerritorialCorporateBodyOrAdministrativeUnit">
        <rdf:type rdf:resource="&owl;Class"/>
        <rdf:type rdf:resource="&sh;NodeShape"/>
        <sh:property>
            <rdf:Description>
                <sh:path rdf:resource="&wdrs;describedby"/>
                <sh:property>
                    <rdf:Description>
                        <sh:path rdf:resource="&dcterms;license"/>
                        <sh:nodeKind rdf:resource="&sh;IRI"/>
                        <sh:pattern>http://creativecommons.org/publicdomain/</sh:pattern>
                    </rdf:Description>
                </sh:property>
                <sh:property>
                    <rdf:Description>
                        <sh:path rdf:resource="&dcterms;modified"/>
                        <sh:datatype rdf:resource="&xsd;dateTime"/>                
                    </rdf:Description>
                </sh:property>
            </rdf:Description>
        </sh:property>
        <sh:property>
            <rdf:Description>
                <sh:path rdf:resource="&gndo;gndIdentifier"/>
                <sh:datatype rdf:resource="&xsd;string"/>                
            </rdf:Description>
        </sh:property>
        <sh:property>
            <rdf:Description>
                <sh:path rdf:resource="&owl;sameAs"/>
                <sh:node rdf:resource="&wikibase;Item"/>                
            </rdf:Description>
        </sh:property>
        <sh:property>
            <rdf:Description>
                <sh:path rdf:resource="&geo;hasGeometry"/>
                <sh:property>
                    <rdf:Description>
                        <sh:path rdf:resource="&geo;asWKT"/>
                        <sh:datatype rdf:resource="&geo;wktLiteral"/>                
                    </rdf:Description>
                </sh:property>
                <sh:property>
                    <rdf:Description>
                        <sh:path rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                        <sh:or rdf:nodeID="_geoType-1"/>                        
                    </rdf:Description>
                </sh:property>                
            </rdf:Description>
        </sh:property>
        <sh:property>
            <rdf:Description>
                <sh:path rdf:resource="&gndo;geographicAreaCode"/>                
                <sh:nodeKind rdf:resource="&sh;IRI"/>
                <sh:pattern>http://d-nb.info/standards/vocab/gnd/geographic-area-code#</sh:pattern>
            </rdf:Description>
        </sh:property>
        <sh:property>
            <rdf:Description>
                <sh:path rdf:resource="&gndo;variantNameForThePlaceOrGeographicName"/>
                <sh:datatype rdf:resource="&xsd;string"/>
            </rdf:Description>
        </sh:property>
        <sh:property>
            <rdf:Description>
                <sh:path rdf:resource="&gndo;preferredNameForThePlaceOrGeographicName"/>
                <sh:datatype rdf:resource="&xsd;string"/>
            </rdf:Description>
        </sh:property>
        <!-- Rule START -->
        <sh:rule>
            <sh:TripleRule>
                <sh:condition rdf:resource="http://d-nb.info/standards/elementset/gnd#TerritorialCorporateBodyOrAdministrativeUnit"/>
                <sh:subject rdf:resource="http://d-nb.info/gnd/4076982-3"/>                
                <sh:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                <sh:object rdf:resource="http://d-nb.info/standards/elementset/gnd#TerritorialCorporateBodyOrAdministrativeUnit"/>
            </sh:TripleRule>
        </sh:rule>
        <sh:rule>
            <sh:TripleRule>
                <sh:condition rdf:resource="http://d-nb.info/standards/elementset/gnd#TerritorialCorporateBodyOrAdministrativeUnit"/>
                <sh:subject rdf:resource="http://d-nb.info/gnd/4076982-3"/>
                <sh:predicate rdf:resource="wdrs:describedby"/>
                <sh:object>
                    <rdf:Description>
                        <sh:path rdf:resource="&wdrs;describedby"/>
                    </rdf:Description>
                </sh:object>
            </sh:TripleRule>
        </sh:rule>
        <sh:rule>
            <sh:TripleRule>
                <sh:condition rdf:resource="http://d-nb.info/standards/elementset/gnd#TerritorialCorporateBodyOrAdministrativeUnit"/>
                <sh:subject rdf:resource="http://d-nb.info/gnd/4076982-3"/>
                <sh:predicate rdf:resource="gndo:gndIdentifier"/>
                <sh:object rdf:datatype="http://www.w3.org/2001/XMLSchema#string">4076982-3</sh:object>
            </sh:TripleRule>
        </sh:rule>
        <sh:rule>
            <sh:TripleRule>
                <sh:condition rdf:resource="http://d-nb.info/standards/elementset/gnd#TerritorialCorporateBodyOrAdministrativeUnit"/>
                <sh:subject rdf:resource="http://d-nb.info/gnd/040769828/about"/>
                <sh:predicate rdf:resource="dcterms:license"/>
                <sh:object>
                    <rdf:Description>
                        <sh:path rdf:resource="dcterms:license"/>
                    </rdf:Description>
                </sh:object>
            </sh:TripleRule>
        </sh:rule>
        
        <!-- Rule END -->
    </rdf:Description>
    <rdf:Description rdf:nodeID="_geoType-1">
        <rdf:first>
            <rdf:Description>
                <sh:hasValue rdf:resource="&sf;Point"/>
            </rdf:Description>
        </rdf:first>
        <rdf:rest rdf:nodeID="_geoType-2"/>
    </rdf:Description>
    <rdf:Description rdf:nodeID="_geoType-2">
        <rdf:first>
            <rdf:Description>
                <sh:hasValue rdf:resource="&sf;Curve"/>
            </rdf:Description>
        </rdf:first>
        <rdf:rest rdf:nodeID="_geoType-3"/>
    </rdf:Description>
    <rdf:Description rdf:nodeID="_geoType-3">
        <rdf:first>
            <rdf:Description>
                <sh:hasValue rdf:resource="&sf;Surface"/>
            </rdf:Description>
        </rdf:first>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
    
    
    <!-- GND DataSet END -->
    
    <!-- Mash DataSets END -->    

</rdf:RDF>


