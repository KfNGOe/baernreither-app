@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix schema: <http://schema.org/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix geonames: <http://www.geonames.org/ontology#> .
@prefix gndo: <http://d-nb.info/standards/elementset/gnd#> .
@prefix gnd: <http://d-nb.info/standards/elementset/gnd#> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix kfngoeo: <https://github.com/KfNGOe/kfngoeo#> .
@prefix kfngoei: <https://github.com/KfNGOe/kfngoei/> .



#kfngoei
kfngoei:nnnnnn
    #:_Annoxxx = class of an Annotation Level of a project
    #_:Annoxxx subClass of dhpluso:Annotation,
    # _: placeholder for project ontology namespace e.g. ebnero, mhdbdbo
    a kfngoeo:AnnoTextComparison ;

    #administrative
    #description of annotation level
    rdfs:label "Annotation level of text comparison"@en ;
    
    #relation to source = the annotation resource
    #text annotation compared
    kfngoeo:source [ #subprop of schema:identifier
        a kfngoeo:PropertyValue ; #subprop of schema:PropertyValue
        kfngoeo:propertyID "TargetText"^^xsd:string ; #subprop of schema:propertyID
        kfngoeo:value "nnnn"^^xsd:string ; #subprop of schema:value
    ] ; #subprop of dct:source

    kfngoeo:source [ #subprop of schema:identifier
        a kfngoeo:PropertyValue ; #subprop of schema:PropertyValue
        kfngoeo:propertyID "BodyText"^^xsd:string ; #subprop of schema:propertyID
        kfngoeo:value "nnnn"^^xsd:string ; #subprop of schema:value
    ] ; #subprop of dct:source

    #key identifier for type of comparison
    rdfs:isDefinedBy ""^^xsd:string ; #equal, notEqual, missing

    #relation to target = the resource that is annotated
    #text content to be compared
    kfngoeo:hasTarget [
        oa:hasSource "nnnnn"^^xsd:string ; #ID of target text
        oa:hasSelector [
            a oa:TextPositionSelector ;
            oa:start nnnn^^xsd:decimal ; #pos nr of tei:anchor/
            oa:end nnnn^^xsd:decimal #pos nr of tei:/app
        ] 
    ] ;

    #relation to body = the annotation itself
    #text content compared 
    kfngoeo:hasBody [
        a kfngoeo:TextualBody;
        rdf:value "nnnnn"@de ;
    ] ;
    kfngoeo:hasBody [
        oa:hasSource "nnnnn"^^xsd:string ; #ID of body text
    ]     
.