  kfngoeo:hasBody _:node200120 .

_:node200118 kfngoeo:hasSource "Bae_TB_8";
  kfngoeo:hasSelector _:node200119 .

_:node200119 a kfngoeo:TextPositionSelector;
  kfngoeo:start 3279;
  kfngoeo:end 3317 .

_:node200120 a kfngoeo:TextualBody;
  kfngoeo:hasSource "Bae_MF_6-2_1242";
  rdf:value "Der Schluss, den Czernin in diesem Briefe aus der Audienz Coudenhoves zieht, ist, wie es sich bald herausstellte, voreilig gewesen. Der Kaiser hatte zu den Ausführungen Coudenhoves geschwiegen, mehr nicht. Beck leugnet, von so einem Schwanken des Kaisers irgendetwas gewußt zu haben und sein Auftreten war nicht danach angetan, eine solche Wendung vorzubereiten. Er ging im Gegenteil gerade und mit allen Mitteln auf das allgemeine Stimmrecht los.";
  rdfs:isDefinedBy "notEqual" .

