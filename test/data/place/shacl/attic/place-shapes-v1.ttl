@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix wdrs: <http://www.w3.org/2007/05/powder-s#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@prefix cc: <http://creativecommons.org/ns#> .

<https://dhplus.sbg.ac.at/shapes/place/>
  a <http://owl.org/Ontology> ;
  dc11:title "dhPLUS Place SHACL" ;
  dc11:creator "Richard HÃ¶rmann" ;
  owl:comment "This is a SHACL graph for validating the dhPLUS OWL ontology."@en ;
  owl:imports <http://njh.me/> ;
  sh:entailment <http://www.w3.org/ns/entailment/RDFS> .

<http://njh.me/#wktLiteral>
  a rdfs:Datatype ;
  dc11:date "2011-06-16"^^xsd:date ;
  rdfs:comment """
            A Well-known Text serialization of a geometry object.
        """@en ;
  dc11:description """
            A Well-known Text serialization of a geometry object.
        """@en ;
  skos:definition """
            A Well-known Text serialization of a geometry object.
        """@en ;
  rdfs:isDefinedBy <http://njh.me/>, <http://www.opengis.net/spec/geosparql/1.0> ;
  skos:prefLabel "Well-known Text Literal"@en ;
  dc11:creator "OGC GeoSPARQL 1.0 Standard Working Group" ;
  rdfs:label "Well-known Text Literal"@en ;
  dc11:contributor "Matthew Perry" .

<https://dhplus.sbg.ac.at/place/E53_Place>
  a sh:NodeShape ;
  sh:property [
    sh:path <https://dhplus.sbg.ac.at/place/P87_is_identified_by> ;
    sh:or (
     _:genid3
     _:genid5
   )
  ] .

<https://dhplus.sbg.ac.at/place/E48_Place_Name>
  a sh:NodeShape ;
  sh:property [
    sh:path <https://dhplus.sbg.ac.at/place/label> ;
    sh:datatype rdf:langString ;
    sh:maxCount "1" ;
    sh:languageIn "de" ;
    sh:equals rdfs:label
  ], [
    sh:path <https://dhplus.sbg.ac.at/place/label> ;
    sh:datatype rdf:langString ;
    sh:maxCount "1" ;
    sh:languageIn "en" ;
    sh:equals rdfs:label
  ] ;
  sh:not <https://dhplus.sbg.ac.at/place/E47_Spatial_Coordinates> .

<https://dhplus.sbg.ac.at/place/PlaceOrGeographicName>
  a sh:NodeShape ;
  sh:not <https://dhplus.sbg.ac.at/place/Geometry> .

<https://dhplus.sbg.ac.at/place/DefinedPlaceOrGeographicName>
  a sh:NodeShape ;
  sh:not <https://dhplus.sbg.ac.at/place/ReplacedPlaceOrGeographicName> .

<https://dhplus.sbg.ac.at/place/ReplacedPlaceOrGeographicName>
  a sh:NodeShape ;
  sh:not <https://dhplus.sbg.ac.at/place/DefinedPlaceOrGeographicName> .

<https://dhplus.sbg.ac.at/place/E47_Spatial_Coordinates>
  a sh:NodeShape ;
  sh:not <https://dhplus.sbg.ac.at/place/E48_Place_Name> .

<https://dhplus.sbg.ac.at/place/Geometry>
  a sh:NodeShape ;
  sh:property [
    sh:path <https://dhplus.sbg.ac.at/place/hasGeometry> ;
    sh:or (
     _:genid10
     _:genid12
     _:genid14
   )
  ] ;
  sh:not <https://dhplus.sbg.ac.at/place/PlaceOrGeographicName> .

<https://dhplus.sbg.ac.at/place/Point>
  a sh:NodeShape ;
  sh:not <https://dhplus.sbg.ac.at/place/Curve>, <https://dhplus.sbg.ac.at/place/Surface> .

<https://dhplus.sbg.ac.at/place/Curve>
  a sh:NodeShape ;
  sh:not <https://dhplus.sbg.ac.at/place/Point>, <https://dhplus.sbg.ac.at/place/Surface> .

<https://dhplus.sbg.ac.at/place/Surface>
  a sh:NodeShape ;
  sh:not <https://dhplus.sbg.ac.at/place/Point>, <https://dhplus.sbg.ac.at/place/Curve> .

<https://dhplus.sbg.ac.at/place/Gnd>
  a sh:NodeShape ;
  sh:property [
    sh:path <https://dhplus.sbg.ac.at/place/hasPart> ;
    sh:node <http://d-nb.info/standards/elementset/gnd#PlaceOrGeographicName>
  ] .

<http://d-nb.info/standards/elementset/gnd#PlaceOrGeographicName>
  a sh:NodeShape ;
  sh:property [
    sh:path wdrs:describedBy ;
    sh:node [
      a sh:NodeShape ;
      sh:targetObjectsOf wdrs:describedBy ;
      sh:property [
        sh:path dc:license ;
        sh:nodeKind sh:IRI ;
        sh:pattern "http://creativecommons.org/publicdomain/"
      ], [
        sh:path dc:modified ;
        sh:datatype xsd:dateTime
      ]
    ]
  ], [
    sh:path <http://d-nb.info/standards/elementset/gnd#gndIdentifier> ;
    sh:datatype xsd:string
  ], [
    sh:path owl:sameAs ;
    sh:node <http://wikiba.se/ontology#Item>
  ], [
    sh:path <http://www.opengis.net/ont/geosparql#hasGeometry> ;
    sh:node [ sh:property [
        sh:path <http://www.opengis.net/ont/geosparql#asWKT> ;
        sh:datatype <http://njh.me/#wktLiteral>
      ], [
        sh:path rdf:type ;
        sh:or (
         _:genid30
         _:genid32
         _:genid34
       )
      ] ]
  ], [
    sh:path <http://d-nb.info/standards/elementset/gnd#geographicAreaCode> ;
    sh:value <http://d-nb.info/standards/vocab/gnd/geographic-area-code#GeographicAreaCodeValue>
  ], [
    sh:path <http://d-nb.info/standards/elementset/gnd#variantNameForThePlaceOrGeographicName> ;
    sh:datatype xsd:string
  ], [
    sh:path <http://d-nb.info/standards/elementset/gnd#preferredNameForThePlaceOrGeographicName> ;
    sh:datatype xsd:string
  ] .

<https://dhplus.sbg.ac.at/place/WikiData>
  a sh:NodeShape ;
  sh:property [
    sh:path <https://dhplus.sbg.ac.at/place/hasPart> ;
    sh:node schema:Dataset
  ], [
    sh:path <https://dhplus.sbg.ac.at/place/hasPart> ;
    sh:node <http://wikiba.se/ontology#Item>
  ] .

schema:Dataset
  a sh:NodeShape ;
  sh:property [
    sh:path schema:about ;
    sh:node <http://wikiba.se/ontology#Item>
  ], [
    sh:path cc:license ;
    sh:nodeKind sh:IRI ;
    sh:pattern "http://creativecommons.org/publicdomain/"
  ], [
    sh:path schema:dateModified ;
    sh:datatype xsd:dateTime
  ] .

<http://wikiba.se/ontology#Item>
  a sh:NodeShape ;
  sh:property [
    sh:path rdfs:label ;
    sh:datatype rdf:langString ;
    sh:maxCount "1" ;
    sh:languageIn "de"
  ], [
    sh:path rdfs:label ;
    sh:datatype rdf:langString ;
    sh:maxCount "1" ;
    sh:languageIn "en"
  ], [
    sh:path <http://www.wikidata.org/prop/direct/P625> ;
    sh:datatype <http://njh.me/#wktLiteral> ;
    sh:maxCount "2"
  ], [
    sh:path <http://www.wikidata.org/prop/direct/P2044> ;
    sh:datatype xsd:decimal ;
    sh:maxCount "2"
  ] .

<https://dhplus.sbg.ac.at/place/ProjectSpecific>
  a sh:NodeShape ;
  sh:property [
    sh:path <https://dhplus.sbg.ac.at/place/hasPart> ;
    sh:node <https://dhplus.sbg.ac.at/place/ReplacedPlaceOrGeographicName>
  ] .

_:genid3 sh:class <https://dhplus.sbg.ac.at/place/E48_Place_Name> .
_:genid5 sh:class <https://dhplus.sbg.ac.at/place/E47_Spatial_Coordinates> .
_:genid10 sh:class <https://dhplus.sbg.ac.at/place/Point> .
_:genid12 sh:class <https://dhplus.sbg.ac.at/place/Curve> .
_:genid14 sh:class <https://dhplus.sbg.ac.at/place/Surface> .
_:genid30 sh:class <http://www.opengis.net/ont/sf#Point> .
_:genid32 sh:class <http://www.opengis.net/ont/sf#Curve> .
_:genid34 sh:class <http://www.opengis.net/ont/sf#Surface> .
